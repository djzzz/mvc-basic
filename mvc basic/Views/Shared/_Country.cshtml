@model mvc_basic.Models.Countrys.Country
<div class="Container">
    <a onclick="deletePerson('Country', '@Model.CountryId')">@Model.Name</a>
    <br />
    @if (Model.Cities.Count != 0)
    {
        <p>Cities:</p>
        @foreach (var city in Model.Cities)
        {
            <p>@city.Name</p>
        }

    }
    <br />
    <br />
</div>